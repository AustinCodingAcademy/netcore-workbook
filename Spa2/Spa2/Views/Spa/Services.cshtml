@model Spa2.Models.ServiceProvider

@{
    ViewData["Title"] = "Services";
}

<h2>Service Providers</h2>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="View">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Id" class="control-label"></label>
                @*<input asp-for="Id" class="form-control" />*@
                <span asp-validation-for="Id" class="text-danger"></span>
            </div>
            <br />

            <div class="form-group">
                <label asp-for="serviceprovider" class="control-label">Service Provider</label>
                <input asp-for="@Model.serviceprovider" class="form-control" />
                <span asp-validation-for="serviceprovider" class="text-danger"></span>
            </div>
            <br />

            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>
<br />

<div>
    <a asp-action="CustomerName">Back to Customer</a>
</div>

